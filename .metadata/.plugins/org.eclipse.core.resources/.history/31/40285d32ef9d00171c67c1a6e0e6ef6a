package beakjun;

import java.util.Arrays;
import java.util.HashMap;
import java.util.Scanner;
import java.util.Stack;

public class Main {
	public static void main(String[] args) {
		int []arr = {6, 20, 34, 8, 38, 40};
		//각 점에서 가까운 두 점을 출력.
		//두 점의 사이가 같은 수가 있다면 합이 최소가 되는 것으로
		
		//풀이 sort 후,max로 돌린다.
		Arrays.sort(arr);
		
		int min = -1;
		int minIdx = 0;
		
		for(int i = 0; i< arr.length; i++) {
			int v = arr[i+1] - arr[i];
			if(v < min | min == -1) {
				min = v;
				minIdx = i;
			}
		}
		System.out.println(arr[minIdx] + " " + arr[minIdx+1]);
		
		
	}
}
